services:
  wp.resume.managementapi:
    build:
      context: C:\Git\WpWebCore\WpCoreSolution
      dockerfile: ResumeManagement/Wp.Resume.ManagementApi/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Wp.Resume.ManagementApi
      target: base
    container_name: Wp.Resume.ManagementApi
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: wpresumemanagementapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/Wp.Resume.ManagementApi.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 5500
      target: 5500
    tty: true
    volumes:
    - C:\Git\WpWebCore\WpCoreSolution\ResumeManagement\Wp.Resume.ManagementApi:/app:rw
    - C:\Users\nasir\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\nasir\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\nasir\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\nasir\.nuget\packages:/root/.nuget/packages:ro
    - C:\Git\WpWebCore\WpCoreSolution:/src:rw
version: '3.4'